<Window x:Class="SR12_2020_POP2021.Prozori.InstruktoriWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SR12_2020_POP2021.Prozori"
        mc:Ignorable="d"
        Title="InstruktoriWindow" Height="450" Width="800">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="28" VerticalAlignment="Top" Width="794" Margin="0,0,-0.4,0">
            <MenuItem Name="miInstruktori" Header="Instruktori">
                <MenuItem Name="miDodajInstruktora" Header="Dodaj instruktora" Click="miDodajInstruktora_Click"></MenuItem>
                <MenuItem Name="miIzmeniInstruktora" Header="Izmeni instruktora" Click="miIzmeniInstruktora_Click"></MenuItem>
                <MenuItem Name="miIzbrisiInstruktora" Header="Izbrisi instruktora" Click="miIzbrisiInstruktora_Click"></MenuItem>
                <Separator/>
                <MenuItem Name="miProfil" Header="Profil instruktora"></MenuItem>
            </MenuItem>
        </Menu>
        <DataGrid Name="dgInstruktori" CanUserAddRows="False" HorizontalAlignment="Left" Margin="26,33,0,0" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Top" Width="727" AutoGenerateColumns="True" AutoGeneratingColumn="dgInstruktori_AutoGeneratingColumn"/>
    </Grid>
</Window>
