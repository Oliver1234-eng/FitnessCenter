        <DataGrid Name="dgAdministratori" CanUserAddRows="False" HorizontalAlignment="Left" Margin="26,33,0,0" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Top" Width="727" AutoGenerateColumns="True" AutoGeneratingColumn="dgAdministratori_AutoGeneratingColumn"/>
